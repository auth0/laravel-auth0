{
    "name": "auth0/login",
    "description": "Laravel plugin that helps authenticate with the auth0 service",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "auth0/auth0-php": "^5.1.0",
        "illuminate/contracts": "5.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/facade"
        ],
        "psr-0": {
            "Auth0\\Login\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Auth0\\Login\\LoginServiceProvider"
            ],
            "aliases": {
                "Auth0": "Auth0\\Login\\Facade\\Auth0"
            }
        }
    }
}
